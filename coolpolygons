import turtle
import random

# change background color
turtle.bgcolor("black")

# create turtle
t = turtle.Turtle()
t.speed(10)

# make color list
colors = ["red", "orange", "yellow", "green", "blue", "purple", "pink", "white", "cyan"]

# make spiral web
for x in range(1000):
    t.pencolor(colors[x % len(colors)])   
    t.width((x / 20) + 1)       
    t.forward(x)                         
    t.left(59)                            